{
  "name": "travis-ip-monitor",
  "author": "The AMP HTML Authors",
  "license": "Apache-2.0",
  "version": "1.0.0",
  "description": "A GitHub app that monitors the set of Travis API IP addresses for other apps to authenticate with",
  "main": "app.js",
  "scripts": {
    "start": "node app.js 3000",
    "dev": "nodemon",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ampproject/amp-github-apps.git"
  },
  "bugs": {
    "homepage": "https://github.com/ampproject/amp-github-apps#readme",
    "url": "https://github.com/ampproject/amp-github-apps/issues"
  },
  "devDependencies": {
    "nodemon": "^1.19.1"
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "dependencies": {
    "@google-cloud/common": "2.0.5",
    "@google-cloud/storage": "3.0.4",
    "dotenv": "8.0.0",
    "express": "4.17.1",
    "request": "2.88.0"
  }
}
