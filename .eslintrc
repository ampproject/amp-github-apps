{
  "root": true,
  "extends": [
    "eslint:recommended",
    "google",
    "prettier"
  ],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "env": {
    "es6": true,
    "jest": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": 1,
    "require-atomic-updates": 1
  }
}
