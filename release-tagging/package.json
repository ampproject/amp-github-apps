{
  "name": "amp-release-tagging-bot",
  "description": "A GitHub App that tags merged PRs and closed issues with its associated release",
  "version": "0.0.1",
  "private": true,
  "license": "Apache-2.0",
  "author": "The AMP HTML Authors",
  "repository": {
    "type": "git",
    "url": "https://github.com/ampproject/amp-github-apps.git"
  },
  "engines": {
    "node": "^10.0.0"
  },
  "scripts": {
    "build": "tsc ./src/*.ts --esModuleInterop",
    "dev": "nodemon",
    "start": "probot run ./src/app.js",
    "test": "jest --verbose=false"
  },
  "dependencies": {
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "probot": "9.6.0",
    "typescript": "3.6.4"
  },
  "devDependencies": {
    "@types/bunyan": "1.8.6",
    "@types/express": "4.17.1",
    "@types/jest": "24.0.20",
    "@types/node": "12.11.7",
    "@typescript-eslint/eslint-plugin": "2.6.0",
    "@typescript-eslint/parser": "2.6.0",
    "eslint": "6.6.0",
    "jest": "24.9.0",
    "nodemon": "1.19.4",
    "smee-client": "1.1.0",
    "supertest": "4.0.2",
    "ts-jest": "24.1.0",
    "ts-node": "8.4.1"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  }
}
