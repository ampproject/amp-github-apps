{
  "root": true,
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "env": {
    "es6": true,
    "jest": true,
    "node": true
  },
  "parser": "@typescript-eslint/parser",
  "extends": [
    "prettier/@typescript-eslint"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    },
    "useJSXTextNode": true,
    "project": "./release-tagging/tsconfig.json"
  },
  "rules": {
    "no-alert": 2,
    "no-cond-assign": 2,
    "no-debugger": 2,
    "no-div-regex": 2,
    "no-dupe-keys": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-implicit-coercion": [
      2,
      {
        "boolean": false
      }
    ],
    "no-implied-eval": 2,
    "no-iterator": 2,
    "no-lone-blocks": 2,
    "no-native-reassign": 2,
    "no-redeclare": 2,
    "no-restricted-globals": [
      2,
      "error",
      "event"
    ],
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-throw-literal": 2,
    "no-unused-expressions": 0,
    "no-unused-vars": [
      2,
      {
        "argsIgnorePattern": "^(var_args$|opt_|unused)"
      }
    ],
    "no-useless-call": 2,
    "no-useless-concat": 2,
    "no-undef": 2,
    "no-var": 2,
    "no-warning-comments": [
      2,
      {
        "terms": [
          "do not submit"
        ],
        "location": "anywhere"
      }
    ],
    "object-shorthand": [
      2,
      "properties",
      {
        "avoidQuotes": true
      }
    ],
    "prefer-const": 2,
    "prettier/prettier": 2,
    "radix": 2,
    "require-atomic-updates": 1
  }
}