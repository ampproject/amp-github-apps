    "test": "vitest"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@octokit/core": "6.1.6",
    "@octokit/graphql": "8.2.2",
    "@octokit/plugin-rest-endpoint-methods": "13.5.0",
    "@types/body-parser": "1.19.6",
    "@types/express": "5.0.3",
    "@types/mustache": "4.2.6",
    "body-parser": "1.20.3",
    "dotenv": "16.6.1",
    "express": "4.21.2",
    "google-auth-library": "9.15.1",
    "http-status-codes": "2.3.0",
    "mustache": "4.2.0",
    "node-cache": "5.1.2",
    "node-fetch": "3.3.2",
    "tsx": "4.20.5",
    "typescript": "5.9.2"
  },
  "devDependencies": {
    "@types/node": "22.18.0",
    "vitest": "2.1.9",
    "nock": "13.5.6"
