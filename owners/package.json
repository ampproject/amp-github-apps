{
  "name": "owners",
  "author": "Google Inc.",
  "private": true,
  "version": "0.0.1",
  "repository": "http://github.com/ampproject/amp-github-apps",
  "description": "A web service that suggests approvers for a GitHub pull request based on OWNERS files and enforces reviews by OWNERS as a GitHub status check.",
  "engines": {
    "node": "^12.13.1"
  },
  "scripts": {
    "init": "node scripts/warm_cache.js",
    "dev": "node info_server.js",
    "start": "probot run ./index.js",
    "deploy-tag": "git tag 'deploy-owners-'`date --utc '+%Y%m%d%H%M%S'`",
    "test": "jest",
    "test:watch": "jest --watch --notify --notifyMode=change"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@google-cloud/storage": "4.3.0",
    "@octokit/rest": "16.43.1",
    "express": "4.17.1",
    "highlight": "0.2.4",
    "json5": "2.1.1",
    "lodash": "4.17.15",
    "minimatch": "3.0.4",
    "probot": "9.9.1",
    "sleep-promise": "8.0.1"
  },
  "devDependencies": {
    "jest": "24.9.0",
    "nock": "11.7.2",
    "sinon": "7.5.0",
    "smee-client": "1.1.0",
    "supertest": "4.0.2"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
