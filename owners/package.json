{
  "name": "owners",
  "author": "Google Inc.",
  "private": true,
  "version": "0.0.1",
  "repository": "http://github.com/ampproject/amp-github-apps",
  "description": "A web service that suggests approvers for a GitHub pull request based on OWNERS files and enforces reviews by OWNERS as a GitHub status check.",
  "engines": {
    "node": "^12.13.1"
  },
  "scripts": {
    "lint": "eslint .",
    "fix": "npm run lint -- --fix",
    "init": "node scripts/warm_cache.js",
    "dev": "node info_server.js",
    "start": "probot run ./index.js",
    "deploy-tag": "git tag 'deploy-owners-'`date --utc '+%Y%m%d%H%M%S'`",
    "pretest": "npm run lint",
    "test": "jest",
    "test:watch": "jest --watch --notify --notifyMode=change"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@google-cloud/storage": "5.4.0",
    "@octokit/auth": "2.0.0",
    "@octokit/rest": "18.0.7",
    "express": "4.17.1",
    "highlight": "0.2.4",
    "json5": "2.1.3",
    "lodash": "4.17.20",
    "minimatch": "3.0.4",
    "probot": "10.10.0",
    "sleep-promise": "9.0.0"
  },
  "devDependencies": {
    "eslint": "7.12.1",
    "jest": "26.6.1",
    "nock": "13.0.4",
    "sinon": "9.0.3",
    "smee-client": "1.2.2",
    "supertest": "6.0.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
