{
  "name": "amp-bundle-size-app",
  "description": "A GitHub App that adds a bundle-size check to pull requests",
  "version": "0.0.1",
  "private": true,
  "license": "Apache-2.0",
  "author": "The AMP HTML authors",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/ampproject/amp-github-apps.git"
  },
  "scripts": {
    "start": "tsx ./src/index.ts",
    "dev": "nodemon",
    "deploy": "gcloud --project amp-bundle-size-bot app deploy",
    "deploy-tag": "git tag 'deploy-bundle-size-'`date -u '+%Y%m%d%H%M%S'`",
    "setup-db": "tsx ./src/setup-db.ts",
    "test": "vitest"
  },
  "dependencies": {
    "@octokit/core": "7.0.3",
    "@octokit/plugin-rest-endpoint-methods": "16.0.0",
    "@octokit/plugin-retry": "8.0.1",
    "@octokit/request-error": "7.0.0",
    "@octokit/types": "14.1.0",
    "@probot/octokit-plugin-config": "4.0.0",
    "knex": "3.1.0",
    "minimatch": "10.0.1",
    "node-cache": "5.1.2",
    "pg": "8.13.0",
    "probot": "14.0.2",
    "sleep-promise": "9.1.0",
    "tsx": "4.19.1",
    "typescript": "5.6.3"
  },
  "devDependencies": {
    "@octokit/webhooks": "14.1.3",
    "@types/express": "5.0.0",
    "@types/fancy-log": "2.0.2",
    "@types/node": "22.8.2",
    "@types/supertest": "6.0.2",
    "express": "5.1.0",
    "fancy-log": "2.0.0",
    "vitest-mock-extended": "3.1.0",
    "nodemon": "3.1.7",
    "smee-client": "4.3.1",
    "sqlite3": "5.1.7",
    "supertest": "7.0.0",
    "vitest": "3.2.4"
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  }
}
