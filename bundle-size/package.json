{
  "name": "amp-bundle-size-app",
  "description": "A GitHub App that adds a bundle-size check to pull requests",
  "version": "0.0.1",
  "private": true,
  "license": "Apache-2.0",
  "author": "The AMP HTML authors",
  "repository": {
    "type": "git",
    "url": "https://github.com/ampproject/amp-github-apps.git"
  },
  "engines": {
    "node": "^10.14.1"
  },
  "scripts": {
    "dev": "nodemon",
    "deploy": "gcloud --project amp-bundle-size-bot app deploy",
    "setup-db": "node ./setup-db.js",
    "start": "probot run ./app.js",
    "test": "jest"
  },
  "dependencies": {
    "@octokit/rest": "16.35.0",
    "knex": "0.20.2",
    "pg": "7.14.0",
    "probot": "9.6.6",
    "sleep-promise": "8.0.1"
  },
  "devDependencies": {
    "fancy-log": "1.3.3",
    "jest": "24.9.0",
    "nock": "11.7.0",
    "nodemon": "1.19.4",
    "smee-client": "1.1.0",
    "sqlite3": "4.1.0",
    "supertest": "4.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  }
}
