{
  "name": "amp-bundle-size-app",
  "description": "A GitHub App that adds a bundle-size check to pull requests",
  "version": "0.0.1",
  "private": true,
  "license": "Apache-2.0",
  "author": "The AMP HTML authors",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/ampproject/amp-github-apps.git"
  },
  "scripts": {
    "start": "tsx ./src/index.ts",
    "dev": "nodemon",
    "deploy": "gcloud --project amp-bundle-size-bot app deploy",
    "deploy-tag": "git tag 'deploy-bundle-size-'`date -u '+%Y%m%d%H%M%S'`",
    "setup-db": "tsx ./src/setup-db.ts",
    "test": "vitest"
  },
  "dependencies": {
    "@octokit/core": "6.1.6",
    "@octokit/plugin-rest-endpoint-methods": "13.5.0",
    "@octokit/plugin-retry": "7.2.1",
    "@octokit/request-error": "6.1.8",
    "@octokit/types": "13.10.0",
    "@probot/octokit-plugin-config": "3.0.2",
    "knex": "3.1.0",
    "minimatch": "10.0.3",
    "node-cache": "5.1.2",
    "pg": "8.16.3",
    "probot": "13.4.7",
    "sleep-promise": "9.1.0",
    "tsx": "4.20.5",
    "typescript": "5.9.2"
  },
  "devDependencies": {
    "@octokit/webhooks": "13.9.1",
    "@types/express": "5.0.3",
    "@types/fancy-log": "2.0.2",
    "@types/node": "22.18.0",
    "@types/supertest": "6.0.3",
    "express": "5.2.0",
    "fancy-log": "2.0.0",
    "vitest-mock-extended": "2.0.2",
    "nodemon": "3.1.10",
    "smee-client": "2.0.4",
    "sqlite3": "5.1.7",
    "supertest": "7.1.4",
    "vitest": "2.1.9"
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  }
}
