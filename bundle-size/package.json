{
  "name": "amp-bundle-size-app",
  "description": "A GitHub App that adds a bundle-size check to pull requests",
  "version": "0.0.1",
  "private": true,
  "license": "Apache-2.0",
  "author": "The AMP HTML authors",
  "repository": {
    "type": "git",
    "url": "https://github.com/ampproject/amp-github-apps.git"
  },
  "scripts": {
    "dev": "nodemon",
    "deploy": "gcloud --project amp-bundle-size-bot app deploy",
    "deploy-tag": "git tag 'deploy-bundle-size-'`date -u '+%Y%m%d%H%M%S'`",
    "setup-db": "node ./setup-db.js",
    "start": "probot run ./app.js",
    "test": "jest --reporters=jest-silent-reporter"
  },
  "dependencies": {
    "@octokit/rest": "20.0.2",
    "knex": "3.0.1",
    "minimatch": "9.0.3",
    "node-cache": "5.1.2",
    "pg": "8.11.3",
    "probot": "12.3.3",
    "sleep-promise": "9.1.0"
  },
  "devDependencies": {
    "express": "4.18.2",
    "fancy-log": "2.0.0",
    "jest": "29.7.0",
    "jest-silent-reporter": "0.5.0",
    "nock": "13.4.0",
    "nodemon": "3.0.1",
    "smee-client": "2.0.0",
    "sqlite3": "5.1.6",
    "supertest": "6.3.3"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  }
}
