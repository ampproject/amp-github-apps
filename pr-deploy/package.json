{
  "name": "amp-pr-deploy-bot",
  "description": "A GitHub App that deploys a PR branch when you ask it to",
  "version": "0.0.1",
  "private": true,
  "license": "Apache-2.0",
  "author": "The AMP HTML Authors",
  "repository": {
    "type": "git",
    "url": "https://github.com/ampproject/amp-github-apps.git"
  },
  "scripts": {
    "build": "tsc",
    "dev": "nodemon",
    "gcp-build": "npm run build",
    "start": "probot run ./dist/src/app.js",
    "pretest": "npm run build",
    "test": "jest dist/test/*.js --reporters=jest-silent-reporter",
    "deploy-tag": "git tag 'deploy-pr-deploy-'`date -u '+%Y%m%d%H%M%S'`"
  },
  "dependencies": {
    "@google-cloud/storage": "5.16.0",
    "@octokit/plugin-rest-endpoint-methods": "5.13.0",
    "@octokit/rest": "18.12.0",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "gunzip-maybe": "1.4.2",
    "mime-types": "2.1.34",
    "nock": "13.2.1",
    "node-fetch": "2.6.6",
    "probot": "12.1.1",
    "tar-stream": "2.2.0",
    "typescript": "4.5.2"
  },
  "devDependencies": {
    "@octokit/types": "6.34.0",
    "@octokit/webhooks-types": "4.16.0",
    "@types/bunyan": "1.8.8",
    "@types/express": "4.17.13",
    "@types/gunzip-maybe": "1.4.0",
    "@types/jest": "27.0.3",
    "@types/mime-types": "2.1.1",
    "@types/node": "16.11.10",
    "@types/node-fetch": "2.5.12",
    "@types/supertest": "2.0.11",
    "@types/tar-stream": "2.2.2",
    "jest": "27.3.1",
    "jest-silent-reporter": "0.5.0",
    "nodemon": "2.0.15",
    "smee-client": "1.2.2",
    "ts-jest": "27.0.7",
    "ts-node": "10.4.0"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "nodemonConfig": {
    "exec": "npm start",
    "ext": "ts,json",
    "watch": [
      ".env",
      "."
    ]
  }
}
