{
  "name": "amp-pr-deploy-bot",
  "description": "A GitHub App that deploys a PR branch when you ask it to",
  "version": "0.0.1",
  "private": true,
  "license": "Apache-2.0",
  "author": "The AMP HTML Authors",
  "repository": {
    "type": "git",
    "url": "https://github.com/ampproject/amp-github-apps.git"
  },
  "scripts": {
    "lint": "eslint . --ext .ts",
    "fix": "npm run lint -- --fix",
    "build": "tsc -p . --esModuleInterop",
    "dev": "nodemon",
    "start": "probot run ./dist/src/app.js",
    "prestart": "npm run build",
    "pretest": "npm run build",
    "test": "jest dist/test/*.js --reporters=jest-silent-reporter",
    "deploy-tag": "git tag 'deploy-pr-deploy-'`date -u '+%Y%m%d%H%M%S'`"
  },
  "dependencies": {
    "@google-cloud/storage": "5.3.0",
    "@octokit/rest": "18.0.12",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "gunzip-maybe": "1.4.2",
    "mime-types": "2.1.28",
    "nock": "13.0.5",
    "node-fetch": "2.6.1",
    "probot": "10.10.2",
    "tar-stream": "2.2.0",
    "typescript": "4.0.5"
  },
  "devDependencies": {
    "@types/bunyan": "1.8.6",
    "@types/express": "4.17.9",
    "@types/gunzip-maybe": "1.4.0",
    "@types/jest": "26.0.20",
    "@types/mime-types": "2.1.0",
    "@types/node": "14.14.20",
    "@types/node-fetch": "1.6.9",
    "@types/supertest": "2.0.10",
    "@types/tar-stream": "2.2.0",
    "@typescript-eslint/eslint-plugin": "4.6.1",
    "@typescript-eslint/parser": "4.6.1",
    "eslint": "7.12.1",
    "jest": "26.6.3",
    "jest-silent-reporter": "0.5.0",
    "nodemon": "2.0.7",
    "smee-client": "1.2.2",
    "ts-jest": "26.4.4",
    "ts-node": "9.0.0"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "nodemonConfig": {
    "exec": "npm start",
    "ext": "ts,json",
    "watch": [
      ".env",
      "."
    ]
  }
}
