{
  "name": "amp-pr-deploy-bot",
  "description": "A GitHub App that deploys a PR branch when you ask it to",
  "version": "0.0.1",
  "private": true,
  "license": "Apache-2.0",
  "author": "The AMP HTML Authors",
  "repository": {
    "type": "git",
    "url": "https://github.com/ampproject/amp-github-apps.git"
  },
  "engines": {
    "node": "^12.13.1"
  },
  "scripts": {
    "lint": "eslint . --ext .ts",
    "fix": "npm run lint -- --fix",
    "build": "tsc ./src/*.ts --esModuleInterop",
    "dev": "nodemon",
    "start": "probot run ./src/app.js",
    "pretest": "npm run build",
    "test": "jest --verbose=false"
  },
  "dependencies": {
    "@google-cloud/storage": "5.6.0",
    "@octokit/rest": "18.0.12",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "mime-types": "2.1.27",
    "nock": "13.0.5",
    "probot": "10.18.0",
    "typescript": "4.1.2",
    "unzip-stream": "0.3.1"
  },
  "devDependencies": {
    "@types/bunyan": "1.8.6",
    "@types/express": "4.17.9",
    "@types/jest": "26.0.16",
    "@types/mime-types": "2.1.0",
    "@types/node": "14.14.10",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "4.9.1",
    "@typescript-eslint/parser": "4.9.1",
    "eslint": "7.15.0",
    "jest": "26.6.3",
    "nodemon": "2.0.6",
    "smee-client": "1.2.2",
    "ts-jest": "26.4.4",
    "ts-node": "9.1.1"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  }
}
