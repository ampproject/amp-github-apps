{
  "name": "amp-pr-deploy-app",
  "description": "A GitHub App that deploys a PR branch when you ask it to",
  "version": "0.0.1",
  "private": true,
  "license": "Apache-2.0",
  "author": "The AMP HTML Authors",
  "repository": {
    "type": "git",
    "url": "https://github.com/ampproject/amp-github-apps.git"
  },
  "engines": {
    "node": "10.16.0"
  },
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./src/app.js",
    "test": "jest --verbose=false"
  },
  "dependencies": {
    "@google-cloud/storage": "3.0.3",
    "dotenv": "8.0.0",
    "express": "4.17.1",
    "fancy-log": "1.3.3",
    "mime-types": "2.1.24",
    "probot": "9.2.15",    
    "typescript": "3.5.3",
    "unzip-stream": "0.3.0"
  },
  "devDependencies": {
    "@types/express": "4.17.0",
    "@types/fancy-log": "1.3.1",
    "@types/jest": "24.0.15",
    "@types/mime-types": "2.1.0",
    "@types/node": "12.6.2",
    "@types/supertest": "2.0.8",
    "@typescript-eslint/eslint-plugin": "1.12.0",
    "@typescript-eslint/parser": "1.12.0",
    "eslint": "5.16.0",
    "jest": "24.8.0",
    "nodemon": "1.19.1",
    "smee-client": "1.1.0",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  }
}
