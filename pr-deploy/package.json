{
  "name": "amp-pr-deploy-bot",
  "description": "A GitHub App that deploys a PR branch when you ask it to",
  "version": "0.0.1",
  "private": true,
  "license": "Apache-2.0",
  "author": "The AMP HTML Authors",
  "repository": {
    "type": "git",
    "url": "https://github.com/ampproject/amp-github-apps.git"
  },
  "engines": {
    "node": "^12.13.1"
  },
  "scripts": {
    "build": "tsc ./src/*.ts --esModuleInterop",
    "dev": "nodemon",
    "start": "probot run ./src/app.js",
    "test": "jest --verbose=false"
  },
  "dependencies": {
    "@google-cloud/storage": "4.1.3",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "mime-types": "2.1.25",
    "probot": "9.6.6",
    "typescript": "3.7.2",
    "unzip-stream": "0.3.0"
  },
  "devDependencies": {
    "@types/bunyan": "1.8.6",
    "@types/express": "4.17.2",
    "@types/jest": "24.0.23",
    "@types/mime-types": "2.1.0",
    "@types/node": "12.12.14",
    "@types/supertest": "2.0.8",
    "@typescript-eslint/eslint-plugin": "2.9.0",
    "@typescript-eslint/parser": "2.9.0",
    "eslint": "6.7.2",
    "jest": "24.9.0",
    "nodemon": "2.0.1",
    "smee-client": "1.1.0",
    "supertest": "4.0.2",
    "ts-jest": "24.2.0",
    "ts-node": "8.5.4"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  }
}
