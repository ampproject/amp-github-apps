{
  "name": "amp-test-status-app",
  "description": "A GitHub App that adds a status check for various test types to pull requests",
  "version": "0.0.1",
  "private": true,
  "license": "Apache-2.0",
  "author": "The AMP HTML authors",
  "repository": {
    "type": "git",
    "url": "https://github.com/ampproject/amp-github-apps.git"
  },
  "engines": {
    "node": "^10.15.3"
  },
  "scripts": {
    "dev": "nodemon",
    "deploy": "gcloud --project amp-test-status-bot app deploy",
    "setup-db": "node ./setup-db.js",
    "start": "probot run ./app.js",
    "test": "jest"
  },
  "dependencies": {
    "body-parser": "1.18.3",
    "express": "4.16.4",
    "express-session": "1.15.6",
    "express-validator": "5.3.1",
    "knex": "0.16.3",
    "pg": "7.9.0",
    "passport": "0.4.0",
    "passport-github2": "0.1.11",
    "probot": "7.5.1"
  },
  "devDependencies": {
    "deepcopy": "2.0.0",
    "fancy-log": "1.3.3",
    "jest": "24.7.1",
    "nock": "10.0.6",
    "nodemon": "1.18.10",
    "smee-client": "1.1.0",
    "sqlite3": "4.0.6",
    "supertest": "4.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  }
}
