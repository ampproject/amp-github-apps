{
  "name": "amp-test-status-app",
  "description": "A GitHub App that adds a status check for various test types to pull requests",
  "version": "0.0.1",
  "private": true,
  "license": "Apache-2.0",
  "author": "The AMP HTML authors",
  "repository": {
    "type": "git",
    "url": "https://github.com/ampproject/amp-github-apps.git"
  },
  "engines": {
    "node": "^12.13.1"
  },
  "scripts": {
    "dev": "nodemon",
    "deploy": "gcloud --project amp-test-status-bot app deploy",
    "deploy-tag": "git tag 'deploy-test-status-'`date --utc '+%Y%m%d%H%M%S'`",
    "lint": "eslint .",
    "fix": "npm run lint -- --fix",
    "setup-db": "node ./setup-db.js",
    "start": "probot run ./app.js",
    "pretest": "npm run lint",
    "test": "jest"
  },
  "dependencies": {
    "express": "4.17.1",
    "knex": "0.21.8",
    "pg": "8.4.2",
    "probot": "10.8.0"
  },
  "devDependencies": {
    "deepcopy": "2.1.0",
    "eslint": "7.12.1",
    "fancy-log": "1.3.3",
    "jest": "26.6.1",
    "nock": "13.0.4",
    "nodemon": "2.0.6",
    "smee-client": "1.2.2",
    "sqlite3": "5.0.0",
    "supertest": "6.0.0"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  }
}
