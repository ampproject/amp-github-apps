{
  "name": "amp-test-case-reporting",
  "author": "The AMP HTML Authors",
  "version": "0.0.1",
  "private": true,
  "repository": "http://github.com/ampproject/amp-github-apps",
  "description": "An app that tracks the pass/fail status of tests run in Travis builds",
  "engines": {
    "node": "12.20.0"
  },
  "scripts": {
    "lint": "eslint **/*.ts",
    "fix": "eslint **/*.ts --fix",
    "lint-conflicts": "eslint --print-config app.ts | eslint-config-prettier-check",
    "build": "tsc",
    "build:watch": "tsc -w --p tsconfig.json",
    "setup-db": "node dist/src/setup_db.js",
    "start": "node dist/index.js",
    "dev": "nodemon",
    "test": "jest",
    "test:watch": "jest --watch --notify --notifyMode=change",
    "deploy-tag": "git tag 'deploy-test-case-reporting-'`date -u '+%Y%m%d%H%M%S'`"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "body-parser": "1.19.0",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "http-status-codes": "2.1.4",
    "knex": "0.21.15",
    "md5": "2.3.0",
    "mustache": "4.0.1",
    "node-fetch": "2.6.1",
    "pg": "8.5.1"
  },
  "devDependencies": {
    "@types/express": "4.17.9",
    "@types/express-serve-static-core": "4.17.17",
    "@types/jest": "26.0.19",
    "@types/knex": "0.16.1",
    "@types/md5": "2.2.1",
    "@types/mustache": "4.0.1",
    "@types/node": "13.13.38",
    "@types/node-fetch": "2.5.7",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "4.9.1",
    "eslint": "7.15.0",
    "eslint-config-prettier": "7.0.0",
    "eslint-plugin-prettier": "3.2.0",
    "jest": "26.6.3",
    "nodemon": "2.0.6",
    "prettier": "2.2.1",
    "sqlite3": "5.0.0",
    "supertest": "6.0.1",
    "ts-jest": "26.4.4",
    "ts-node": "9.1.1",
    "typescript": "4.1.3"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dist/"
    ]
  }
}
