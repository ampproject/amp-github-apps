{
  "root": true,
  "extends": ["eslint:recommended", "google", "prettier"],
  "plugins": ["prettier"],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 6,
    "requireConfigFile": false,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true,
      "arrowFunctions": true,
      "blockBindings": true,
      "forOf": false,
      "destructuring": true,
      "spread": true
    }
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "globals": {
    "d3": false
  }
}
